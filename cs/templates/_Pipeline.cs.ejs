<% 

    if (!locals.abstractProcessorClassName) {
        locals.abstractProcessorClassName = "MyProcessor"; 
    }
    
    if (!locals.namespace) {
        locals.namespace = "MyNamespace"; 
    }
%>

using System.Collections.Generic;
using MyNamespace.Processors;

namespace <%= namespace %>
{
    public class <%= name %>
    {
        public static readonly <%= name %> Instance = new <%= name %>();
        public IEnumerable<<%= abstractProcessorClassName %>> GetProcessors()
        {
            <% processors.forEach(function(processor){ %>yield return <%= processor %>.Instance;<% }); %>
        }
    }
}