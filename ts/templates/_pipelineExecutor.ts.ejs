import { PipelineRunner } from "solid-pipelines";
import { <%= argumentsClassName %> } from './<%= argumentsFileName %>'
import { <%= pipelineClassName %> } from './<%= pipelineFileName %>'

export class <%= name %> {
    public static Instance: <%= name %> = new <%= name %>();

    execute(args: <%= argumentsClassName %>) : Promise<void> {
        var runner:PipelineRunner = new PipelineRunner();

        return runner.RunPipeline(<%= pipelineClassName %>.Instance, args);
    }
}