<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <objects>
            <pipelineExecutors>
                <<%= name %> type="MyNamespace.<%= name %>Executor, MyLibrary">
                    <param name="pipelineName"><%= name %></param>
                </<%= name %>>
            </pipelineExecutors>
        </objects>
        
        <pipelines>
            <<%= name %>>
            <% processors.forEach(function(processor){ %>    
                <processor type="MyNamespace.<%= processor %>, MyLibrary" />
            <% }); %>
            </<%= name %>>
        </pipelines>
    </sitecore>
</configuration>